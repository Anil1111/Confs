
@{
    ViewBag.Title = "EmpByConfType";
}

<h2>EmpByConfType</h2>

@{

    var myChart = new Chart(width: 600, height: 400)
      .AddTitle("Conference By Type")
      .AddSeries(chartType: "Pie",
         xValue: new[] { "Obligatoire"+@ViewBag.oblg, "Facultative"+ @ViewBag.Fact },
         yValues: new[] { @ViewBag.oblg, @ViewBag.Fact })
      .Write();
}